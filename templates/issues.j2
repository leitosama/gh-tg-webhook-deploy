{% if data['action']=="created" %}
    <a href="{{ data['repository']['html_url'] }}">[ {{ data['repository']['name'] }} ] </a>
    <a href="{{ data['sender']['html_url'] }}">{{ data['sender']['login'] }}</a> создал Issue {{ data['issue']['number'] }} <a href="{{ data['issue']['html_url'] }}">{{ data['issue']['title'] }}</a>:
    <code>
    {{ data['issue']['body'] }}
    </code>
{% endif %}